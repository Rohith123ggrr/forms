<script defer integrity="sha384-/LwbeItYhfLT59ofJHOoDoqx/BcDDi5FbIUbqo0oDuz5lpo/5QKILtG1f2XTduw4" src="https://unpkg.com/web-vitals@3.0.4"></script>
<script>
    const sentMetrics = [];
    function sendVital(metric) {
        if (sentMetrics.includes(metric.name)) return;
        sentMetrics.push(metric.name);

        const data = {
            name: metric.name,
            value: metric.value,
            signature: 'webvitals:1puRQZ:Vz0_5OnMSd17Lv7aM69stMWX0Kzi4txpiCEz9aEtv8Q',
            referer_path: window.location.href,
            platform: 'templates'
        };

        if (navigator.sendBeacon) {
            navigator.sendBeacon('/collect/web-vitals/', JSON.stringify(data));
        } else {
            $.post('/collect/web-vitals/', data);
        }
    }

    if (Math.random() * 100 <= 1.0) {
        addEventListener('DOMContentLoaded', function() {
            webVitals.getCLS(sendVital);
            webVitals.getFID(sendVital);
            webVitals.getLCP(sendVital);
            webVitals.getFCP(sendVital);
            webVitals.getTTFB(sendVital);
            webVitals.getINP(sendVital);
        });
    }
</script>
<script>
        var cat_base_url = "/images/search/background/?cat=val";
        
        $('.header_search').find('input[type="text"], input:checked, input[type="hidden"]').each(function(){
            var i = this;
            $('.add_search_params').each(function(){
                if (i.value && (!$(this).hasClass('filter') || i.name != 'colors' && i.name != 'min_width' && i.name != 'min_height' && i.name != 'q'))
                    $(this).append($('<input type="hidden" name="'+i.name+'">').val(i.value));
            });
        });
        $('.add_search_params').submit(function(){
            // remove unused fields / GET params
            if (!$(this).find('[name="min_width"]').val()) { $(this).find('[name="min_width"]').removeAttr('name'); }
            if (!$(this).find('[name="min_height"]').val()) { $(this).find('[name="min_height"]').removeAttr('name'); }
        });
        
    </script>
